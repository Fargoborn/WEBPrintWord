<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>Action_w</servlet-name>
        <servlet-class>org.mycompany.myname.Action_w</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>Action_sh</servlet-name>
        <servlet-class>org.mycompany.myname.Action_sh</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Action_w</servlet-name>
        <url-pattern>/Action_w</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Action_sh</servlet-name>
        <url-pattern>/Action_sh</url-pattern>
    </servlet-mapping>

</web-app>
